				print( "Error in Deadline Submission Script: could not create " + $submitFilename + "\n" );

			fprint $fileId ( "Plugin=Arnold\n" );
			fprint $fileId ( "Name=" + $singleJobName + " - Render - Region "+$curRegion+"\n" );

			if( !$exportLocal || $submitDependentJob )
